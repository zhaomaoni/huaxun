/* pages/content/content.wxss */
@import "/wxParse/wxParse.wxss";
.navImg{
  width: 100%;
  height: 89.6px;
  position: fixed;
  bottom: 0;
}
.contentBox{
  width: 100%;
  height: auto;
}
.contentTitle{
  width: 90%;
  margin: 0 auto;
  font-weight: 600;
  font-size: 20px;
  line-height: 25px;
  display: block;
  overflow: hidden;
  padding:15px 0;
}
.contentMsg{
  width: 90%;
  margin: 0 auto;
  font-size: 12px;
  color: #999;
  line-height: 25px;
  display: block;
  overflow: hidden;
  padding-bottom: 15px;
  border-bottom: 1px solid #ddd
}
.conTime{
  float: left;
}
.contentMsg image{
  width: 16px;
  height: 16px;
  vertical-align: middle;
  padding-right: 5px
}
.conMain{
  width: 90%;
  margin: 15px auto 90px auto;
  display: block;
  font-size: 16px;
  line-height: 25px;
}
 .conMain image{
  width: 100%;
} 
.conMain .wxParse-p{
  padding: 8px 0;
  font-size:16px;
}
.conMain .wxParse-div{
  width: 100%;
  height: auto;
}
.wxParse-div image{
  width: 100%;
  object-fit: cover
}