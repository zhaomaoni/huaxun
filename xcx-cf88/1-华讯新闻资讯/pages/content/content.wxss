/* pages/content/content.wxss */
.navImg{
  width: 100%;
  height: 89.6px;
  position: fixed;
  bottom: 0;
  border-top: 1px solid #f0f0f0;
  background: #fff;
}
.contentBox{
  width: 100%;
  height: auto;
}
.contentTitle{
  width: 90%;
  margin: 0 auto;
  font-weight: 600;
  font-size: 18px;
  line-height: 25px;
  display: block;
  overflow: hidden;
  padding:15px 0;
}
.contentMsg{
  width: 90%;
  margin: 0 auto;
  font-size: 12px;
  color: #999;
  line-height: 25px;
  display: block;
  overflow: hidden;
}
.conTime{
  float: left;
}
.conCheck{
  float: right;
}
.contentMsg image{
  width: 16px;
  height: 16px;
  vertical-align: middle;
  padding-right: 5px
}
.conMain{
  width: 90%;
  margin: 15px auto;
  display: block;
  font-size: 14px;
  line-height: 25px;
  margin-bottom: 90px;
}
.conMain .wxParse-p{
  padding: 8px 0;
  font-size:16px;
}