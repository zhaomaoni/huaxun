}

/* 支付弹层 */
.pay-layer-box{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.3)
}
.pay-content-box{
  width: 85.3%;
  height: 63%;
  position: fixed;
  left: 7.35%;
  right: 7.35%;
  top: 18.5%;
  bottom: 18.5%;
  background: linear-gradient(#8A37A5, #522E7A); /* 标准的语法 */
  border-radius: 0.25rem;
}
.pay-main-box{
  width: 90%;
  margin: 0 auto;
  height: auto;
  overflow: hidden;
  position: relative;
}
.pag-logo{
  width: 7.25rem;
  height: 3.75rem;
  margin: 1.8rem auto 1rem auto;
}
.pag-logo image{
  width: 100%;
  height: 100%;
}
.pay-title{
  width: 100%;
  height: auto;
  line-height: 1.25rem;
  text-align: center;
  color: #fff;
  font-size: 1.125rem;
}
.pay-title view:last-child{
  font-size: 0.75rem;
  color: #b78ac6
}
.pay-add-minus{
  width: 100%;
  height: 3.125rem;
  margin: 0.625rem auto 0.9rem auto;
  background: url("https://api.51gsl.com/program/StockPush/images/icon_inp.png")no-repeat center;
  background-size: 100% 100%;
}
.pay-minus,.pay-add{
  width: 22%;
  height: 3.125rem;
  float: left
}
.pay-add{
  float: right;
}
.pay-minus image,.pay-add image{
  width: 1.25rem;
  height: 1.25rem;
  display: block;
  margin: 0.9375rem auto;
}
.pay-money-num{
  width: 56%;
  height: 3.125rem;
  overflow: hidden;
  float: left;
}
.pay-money-num input{
  text-align: center;
  height: 100%;
  line-height: 100%;
  color: #653188;
}
.pay-btn-box{
  width: 100%;
  height: 3.4375rem;
  line-height: 3rem;
  text-align: center;
  color: #fff;
  text-shadow: #ff8c01 2px 3px 3px;
  background: url("https://api.51gsl.com/program/StockPush/images/icon_btnbg.png") no-repeat center;
  background-size: 100% 100%;
}
.pay-close{
  position: absolute;
  right: 0;
  top: 3%;
  width: 1rem;
  height: 1rem;
}
.pay-close image{
  width: 100%;
  height: 100%;
}