}
.life-box2 view{
  width: 95%;
  margin: 0 auto;
  float: none
}
.life-btn-box view button{
  width: 100%;
  background: transparent;
  color: #fff;
}


.brokerBox{
  width: 100%;
  height: auto;
  margin: 0 auto;
  overflow: hidden
}
.broker-btn-box{
  width: 100%;
  height: 3.125rem;
  line-height: 3.125rem;
  margin: 0.75rem auto;
  overflow: hidden;
  background-image:linear-gradient(90deg, #F7DA71 0%, #F7941C 100%);
