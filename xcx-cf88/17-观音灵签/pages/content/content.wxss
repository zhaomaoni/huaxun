/* pages/content/content.wxss */
@import "/wxParse/wxParse.wxss";
.page{
  width: 88%;
  margin:0 auto 100px auto;
}
.title{
  padding-top: 10px;
  line-height: 22px;
  font-weight: 600;
  font-size: 18px;
  overflow: hidden
}
.pic{
  width: 190px;
  height: 350px;
  margin: 20px auto;
}
.pic image{
  width: 100%;
  height: 100%;
  vertical-align: middle
}
/*  banner  */
.banner{
  width: 100%;
  height: 89px;
  margin-top: 15px;
  position: fixed;
  bottom: 0;
}
.navImg{
  display: inline
}
.conMain{
  width: 100%;
  height: auto;
  overflow: hidden
}