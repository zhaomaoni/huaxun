/* pages/changexz/changexz.wxss */
page{
  background: url("https://api.51gsl.com/program/Constellation/images/xz_bk.png") no-repeat left center;
  background-size: 100% 100%;
  background-attachment: fixed;
  padding-bottom: 90px;
}
.main{
  width: 100%;
  height: 100%;
  color: #fff
}
.listBox{
  width: 92%;
  height: 78px;
  overflow: hidden;
  margin: 18px auto 0 auto;
  border-bottom: 1px solid #504559;
}
.listPic{
  width: 67px; 
  height: 59px;
  float: left;
  margin-right: 13px;
}
.listPic image{
  width: 100%;
  height: 100%;
}
.listTxt{
  width: auto;
  height: 55px;
  margin: 5px 0;
  overflow: hidden;
}
.title{
  height: 25px;
  line-height: 25px;
}
.title text:first{
  font-size: 16px;
}
.title text:last-child{
  float: right;
  font-size: 11px;
  color: #e5e5e5
}
.content{
  font-size: 12px;
  color: #c2c2c2;
  line-height: 30px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis
}
/*  banner  */
.banner{
  width: 100%;
  height: 89px;
  position: fixed;
  bottom: 0;
  overflow: hidden
}
.navImg{
  width: 100%;
  height: 100%
}