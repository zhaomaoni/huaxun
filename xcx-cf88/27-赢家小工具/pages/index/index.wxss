/* pages/index/index.wxss */
.body{
  width: 100%;
  height: auto;
  font-size: 14px;
  margin: 0;
  padding: 0;
  overflow: hidden;
}
.content{
  width: 90%;
  height: auto;
  margin: 0 auto 10px auto;
  overflow: hidden;
  padding-bottom: 20px;
  padding-top: 10px;
  border-top: 0.5px solid #d7d7d7
}
.content:last-child{
  border-bottom: 0;
  margin-bottom: 90px;
}
.title{
  width: 100%;
  height: 15px;
  line-height: 15px;
  padding-left: 5px;
  margin-bottom: 10px;
  border-left: 3px solid #e3383a;
}
.Main{
  width: 30%;
  height: auto;
  float: left;
  margin: 5px 1.6% 15px 1.6%;
  overflow: hidden;
}
.dlImg{
  width:15vw; 
  height:15vw;
  margin: 0 auto;
  border-radius: 100px;
  overflow: hidden;
  border: 1px solid #b7b7b7
}
.dlImg image{
  width: 100%;
  height: 100%;
}
.ddTxt{
  width: 100%;
  height: 30px;
  color: #666;
  font-size: 12px;
  line-height: 30px;
  text-align: center
}
/*  banner  */
.banner{
  width: 100%;
  height: 89px;
  position: fixed;
  bottom: 0;
  border-top: 1px solid #ccc;
  overflow: hidden
}
.navImg{
  width: 100%;
  height: 100%
}